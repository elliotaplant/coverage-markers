{
  "name": "coverage-markers",
  "main": "index.js",
  "version": "0.5.0",
  "description": "Atom package to display JavaScript test coverage on gutter of editor.",
  "keywords": [
    "atom",
    "javascript",
    "coverage",
    "lcov"
  ],
  "activationCommands": {
    "atom-workspace": "coverage-markers:toggle"
  },
  "repository": "https://github.com/kentaro-m/coverage-markers",
  "homepage": "https://github.com/kentaro-m/coverage-markers",
  "bugs": {
    "url": "https://github.com/kentaro-m/coverage-markers/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "atom --test test/"
  },
  "atomTestRunner": "./test/runner",
  "atomTranspilers": [
    {
      "glob": "{lib,test}/**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "cacheKeyFiles": [
          "package.json",
          ".babelrc"
        ]
      }
    }
  ],
  "dependencies": {
    "@cedx/lcov": "^4.0.0",
    "bluebird": "^3.5.0",
    "chokidar": "^1.7.0",
    "event-kit": "^2.3.0",
    "fast-glob": "^2.1.0",
    "atom-babel6-transpiler": "^1.1.1",
    "babel-preset-env": "^1.6.1",
    "ajv": "^6.2.0"
  },
  "devDependencies": {
    "atom-mocha-test-runner": "^1.0.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "co-mocha": "^1.2.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^3.4.2",
    "prettier": "^1.10.2",
    "sinon": "^2.3.6"
  }
}
